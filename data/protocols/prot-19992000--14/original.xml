<protocol id="prot-19992000--14">
  <contentBlock ix="0">
    <textBlock ix="0">
      <textBlock ix="0">Det justerade protokollet beräknas utkomma om 3 veckor</textBlock>
    </textBlock>
  </contentBlock>
  <contentBlock ix="1">
